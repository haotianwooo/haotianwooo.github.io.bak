      <div class="tmy-panel tmy-panel-bordered">
          <form name="loginForm" class="loginForm" id="login-page" ng-controller="loginCtrl" ng-submit="login()" ng-cloak  data-js="account/loginInWechat">
    <h3 class="form-header">登录到天码营</h3>
    <hr>
    <fieldset>
      <div class="form-group">
        <div class="input-group input-group-sm">
          <span class="input-group-addon">
            <i class="fa fa-user"></i>
          </span>
          <input id="username" name="username" type="text" placeholder="用户名" class="form-control input-md"
                 required autofocus
                 ng-model="user.username">
        </div>

        <div class="alert alert-danger"
             ng-show="loginForm.username.$error.required && loginForm.username.$dirty">
          用户名不能为空
        </div>
      </div>
      <div class="form-group">
        <div class="input-group input-group-sm">
          <span class="input-group-addon">
            <i class="fa fa-lock"></i>
          </span>
          <input id="password" name="password" type="password" placeholder="密码" class="form-control input-md"
                 required
                 ng-model="user.password">
        </div>
        <div class="alert alert-danger"
             ng-show="loginForm.password.$error.required && loginForm.password.$dirty">
          密码不能为空
        </div>
      </div>
      <div class="form-group form-btns">
        <div class="alert alert-danger" ng-show="msg" ng-bind="msg"></div>
        <div class="pull-right">
          <a class="btn btn-default btn-sm btn-wide" href="/account/register?next=http%3A%2F%2Fcourse.tianmaying.com%2Fnode%2Flesson%2Fbootstrap%2Frepo%2Fblob%2Fbootstrap%2Fcatty.jpeg">注册</a>
          <button class="btn btn-primary btn-sm btn-wide" ng-hide="busy" type="submit">登录</button>
          <a class="btn btn-primary btn-sm btn-wide" ng-show="busy" disabled>
            <i class="fa fa-spinner fa-spin"></i> 正在登录
          </a>
        </div>
      </div>
      <div class="clearfix"></div>
      <div class="oauth-block">
        <span class="title">快速登录：</span>
        <a href="https://github.com/login/oauth/authorize?client_id=a1f0cd59f8c429b185ec&amp;redirect_uri=http%3A%2F%2Ftianmaying.com%2Foauth%2Fgithub%2Fcallback&amp;state=1h8k68be244kxdeb2k91he2040bdedd1230f40817feeekjm">
          <i class="fa fa-github"></i>
        </a>
        <a href="https://api.weibo.com/oauth2/authorize?client_id=1502829444&amp;redirect_uri=http%3A%2F%2Ftianmaying.com%2Foauth%2Fweibo%2Fcallback&amp;response_type=code">
          <i class="fa fa-weibo"></i>
        </a>
        <a href="https://graph.qq.com/oauth2.0/authorize?response_type=code&amp;client_id=101236133&amp;redirect_uri=http%3A%2F%2Ftianmaying.com%2Foauth%2Fqq%2Fcallback&amp;state=1h8k68be244kxdeb2k91he2040bdedd1230f40817feeekjm">
          <i class="fa fa-qq"></i>
        </a>
        <a href="https://open.weixin.qq.com/connect/qrconnect?appid=wx10e769bce8def9bb&redirect_uri=http%3A%2F%2Ftianmaying.com%2Foauth%2Fweixin%2Fcallback&response_type=code&state=esfadsgsad34fwdef&scope=snsapi_login#wechat_redirect">
          <i class="fa fa-wechat"></i>
        </a>
      </div>
    </fieldset>
